---
import BaseLayout from "../layouts/BaseLayout.astro";
import Navigation from "../components/global/Navigation.astro";
import Faq from "../components/infopages/Faq.astro";
import Footer from "../components/global/Footer.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!--
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="FAQ"
  description="Keep a record of the time you dedicate to your projects"
  canonical="http://usetrak.com"
  openGraph={{
    url: "http://usetrak.com",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "http://usetrak.com/opengraph/social-og.jpeg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "http://usetrak.com/opengraph/twitter-og.jpeg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "http://usetrak.com/opengraph/social-og.jpeg" },
      { url: "http://usetrak.com/opengraph/twitter-og.jpeg" },
    ],
    site_name: "Trak",
  }}
  twitter={{
    handle: "@usetrakcom",
    site: "@site",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <Navigation />
  <Faq />
  <Footer />
</BaseLayout>
